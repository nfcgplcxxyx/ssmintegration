<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jcfx.mapper.StudentMapper">

    <insert id="insert">
        insert into t_user
        values (#{id}, #{name}, #{age})
    </insert>

    <select id="selectById" resultType="com.jcfx.bean.Student">
        select *
        from t_user
        where id = #{id}
    </select>

    <select id="selectAll" resultType="com.jcfx.bean.Student">
        select *
        from t_user
    </select>
</mapper>